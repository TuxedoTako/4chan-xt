    "futaba",
    "Gfycat",
    "greasemonkey",
    "iframes",
    "Keybinds",
    "Linkify",
    "Liveleak",
    "Pastebin",
    "Peertube",
    "quotelink",
    "quotelinks",
    "Soundcloud",
    "Streamable",
    "Tinyboard",
    "Twitchtv",
    "userscript",
    "userscripts",
    "Vidlii",
    "Vocaroo",
    "yotsuba"
  ],
  "ignoreWords": [
    "tyme",
    "werk"
